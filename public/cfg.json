{
    "dT": "{@this}/deviceTypes.js",

    "components": {

        "login": {
            "active": true,
            "defaultStyle": {
                "desktop": "position: absolute;width: 100%;min-height: 100%;",
                "mobile": "position: absolute;width: 100%;min-height: 100%;"
            },
            "remote_html": {
                "desktop": {
                    "src": "{@this}/comps/login/login.html",
                    "version": ""
                },
                "mobile": {
                    "src": "{@this}/comps/login/login.html",
                    "version": ""
                }
            },
            "remote_css": {
                "mobile": {
                    "src": ["{@this}/comps/login/login.css"],
                    "version": ""
                },
                "desktop": {
                    "src": ["{@this}/comps/login/login.css"],
                    "version": ""
                }

            },
            "remote_js": {
                "desktop": {
                    "src": ["{@this}/comps/login/login.js"],
                    "version": ""
                },
                "mobile": {
                    "src": ["{@this}/comps/login/login.js"],
                    "version": ""
                }
            },

            "position": "application"
        },


